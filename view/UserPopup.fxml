<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.UserPopupController">
   <center>
      <Pane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="text" layoutX="26.0" layoutY="41.0" />
            <Text layoutX="56.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Username" />
            <Button layoutX="46.0" layoutY="88.0" mnemonicParsing="false" onAction="#enterUserName" text="Send" />
            <Button fx:id="closeButton" layoutX="100.0" layoutY="88.0" mnemonicParsing="false" onAction="#exitFile" text="Cancel" />
         </children>
      </Pane>
   </center>
</BorderPane>
